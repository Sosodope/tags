{
  "<state>": {
    "template": "./index.marko",
    "node-factory": "./ensure-component.js",
    "transformer": "./transformer.js",
    "@variable": {
      "type": "expression",
      "autocomplete": [
        {
          "description": "The name of the state variable to control"
        }
      ]
    },
    "autocomplete": [
      {
        "snippet": "state|${1:variable}|",
        "description": "Used to create an inline controllable state"
      },
      {
        "snippet": "state|${1:variable} = ${object}|"
      }
    ]
  }
}
